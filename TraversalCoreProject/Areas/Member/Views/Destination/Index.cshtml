@using EntityLayer.Concrete
@model List<Destination>  

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_MemberLayout.cshtml";
}

<h1>Aktif Tur Rotalarımız</h1>
<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Konum</th>
        <th>Süre</th>
        <th>Kapasite</th>
        <th>Fiyat</th>
        <th>Detaylar</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <th>@item.DestinationID</th>
            <td>@item.City</td>
            <td>@item.DayNight</td>
            <td>@item.Capacity</td>
            <td>@item.Price</td>
            <td><a href="#" class="btn btn-outline-info">Detaylar</a></td>
        </tr>
    }
</table>
